<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="450"
             x:Class="LoginUI.MyUserControls.MyDockPanel">
<!--This User Control for me to remember how to use it-->
	<DockPanel>
		<Border Background="DarkBlue" Height="60" DockPanel.Dock="Top"/>
		<Border Background="DarkBlue" Height="60" DockPanel.Dock="Bottom"/>
		<Border Background="Blue" Width="60" DockPanel.Dock="Right"/>
		<Border Background="Blue" Width="60" DockPanel.Dock="Left"/>
		<Border Background="White">
			<Label>Center</Label></Border>
	</DockPanel>
</UserControl>
