<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
            xmlns:uc="clr-namespace:LoginUI.MyUserControls"
			 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="LoginUI.WinUserControl.RegisterUserControl">

	<Panel>
		<StackPanel Margin="10" >
			<Label FontStyle="Oblique" FontFamily="nice" FontWeight="Black" FontSize="24" Content="Hello And Welcome to my Register UserControl"/>
			<uc:MyTextBlock Content="User Name"/>
			<uc:MyTextBox HorizontalAlignment="Left"/>
			<uc:MyTextBlock Content="Password"/>
			<uc:MyTextBox HorizontalAlignment="Left"/>
			<StackPanel Orientation="Vertical" Margin="20">
				<uc:MyButton  Name="LoginBtn"> </uc:MyButton>
				<uc:MyButton Name="RegisterBtn"> </uc:MyButton>
			</StackPanel>

		</StackPanel>


	</Panel>
</UserControl>
